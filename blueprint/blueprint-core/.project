<projectDescription>
  <name>org.apache.aries.blueprint.core</name>
  <comment>This bundle contains the core implementation of Blueprint 
      along with the &quot;ext&quot; namespace handler. NO_M2ECLIPSE_SUPPORT: Project files created with the maven-eclipse-plugin are not supported in M2Eclipse.</comment>
  <projects>
    <project>org.apache.aries.proxy.api</project>
    <project>org.apache.aries.quiesce.api</project>
    <project>org.apache.aries.util</project>
  </projects>
  <buildSpec>
    <buildCommand>
      <name>org.eclipse.jdt.core.javabuilder</name>
    </buildCommand>
    <buildCommand>
      <name>org.eclipse.ui.externaltools.ExternalToolBuilder</name>
      <arguments>
        <dictionary>
          <key>LaunchConfigHandle</key>
          <value>&lt;project&gt;/.externalToolBuilders/Maven_Ant_Builder.launch</value>
        </dictionary>
      </arguments>
    </buildCommand>
  </buildSpec>
  <natures>
    <nature>org.eclipse.jdt.core.javanature</nature>
  </natures>
</projectDescription>