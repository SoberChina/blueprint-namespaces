<?xml version="1.0" encoding="UTF-8" standalone="no"?>
<xsd:schema xmlns="http://aries.apache.org/ns/test/list"
	xmlns:xsd="http://www.w3.org/2001/XMLSchema" targetNamespace="http://aries.apache.org/ns/test/list"
	elementFormDefault="qualified" attributeFormDefault="unqualified"
	version="1.0.0">
	
	<xsd:element name="list">
		<xsd:complexType>
			<xsd:simpleContent>
				<xsd:extension base="xsd:string">
					<xsd:attribute name="id" type="xsd:ID"/>				
				</xsd:extension>
			</xsd:simpleContent>
		</xsd:complexType>
	</xsd:element>
	
</xsd:schema>